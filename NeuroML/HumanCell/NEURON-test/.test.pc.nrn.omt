# Script for running automated tests on OSB, see https://github.com/OpenSourceBrain/osb-model-validation

target: test_HL5PC.hoc
engine: NEURON
mep: .test.pc.mep
experiments:
  stepPC:
    observables:
      spike times:
        file:
          path: filament_100000042.pyr.v.dat
          columns: [0,1]
          scaling: [1000,1000]
        spike detection:
          method: threshold
        tolerance: 0.00014490653528477428
